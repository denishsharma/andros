<UserControl x:Name="VBcontrol" x:Class="VolumeButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:KLB463"
             mc:Ignorable="d" 
              d:DesignHeight="40" d:DesignWidth="40">
    <Grid>
        <Rectangle x:Name="SBback" Height="40" StrokeThickness="0" Width="40" Fill="#FF6E6E6E" Opacity="0"/>
        <Rectangle x:Name="SBclicked" Height="40" StrokeThickness="0" Width="40" Fill="#FF555555" Opacity="0"/>
        <Image x:Name="SBicon" HorizontalAlignment="Center" Height="18" VerticalAlignment="Center" Width="18" Source="/KLB463;component/Images/icons8-audio-24.png"/>
        <Rectangle x:Name="SBfront" Height="40" StrokeThickness="0" Width="40" Fill="#01000000"/>
    </Grid>
</UserControl>
