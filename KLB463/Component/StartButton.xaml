<UserControl x:Name="SBcont" x:Class="StartButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:KLB463"
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="50">
    <Grid>
        <Rectangle x:Name="SBback" HorizontalAlignment="Left" Height="40" StrokeThickness="0" VerticalAlignment="Top" Width="50" Fill="#FF6E6E6E" Opacity="0"/>
        <Rectangle x:Name="SBclicked" HorizontalAlignment="Left" Height="40" StrokeThickness="0" VerticalAlignment="Top" Width="50" Fill="#FF555555" Opacity="0"/>  
        <Ellipse x:Name="SBicon_hover" HorizontalAlignment="Center" Height="18" Margin="16,11" VerticalAlignment="Center" Width="18" StrokeThickness="3" Stroke="#FF437FFF"/>
        <Ellipse x:Name="SBicon" HorizontalAlignment="Center" Height="18" Margin="16,11" VerticalAlignment="Center" Width="18" StrokeThickness="3" Stroke="White"/>
        <Rectangle x:Name="SBfront" HorizontalAlignment="Left" Height="40" StrokeThickness="0" VerticalAlignment="Top" Width="50" Fill="#01000000"/>
    </Grid>
</UserControl>
