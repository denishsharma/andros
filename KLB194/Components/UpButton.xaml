<UserControl x:Name="UBcontrol" x:Class="UpButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:KLB194"
             mc:Ignorable="d" 
             d:DesignHeight="32" d:DesignWidth="32">
    <Grid>
        <Ellipse x:Name="hoverCircle" Fill="Black" StrokeThickness="0" Opacity="0" Width="32" Height="32"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="22" Margin="5,5,0,0" VerticalAlignment="Top" Width="22" Source="/KLB194;component/Images/icons8-expand-arrow-32.png"/>
        <Rectangle x:Name="frontRect" Width="32" Height="32" Fill="#01000000"/>
    </Grid>
</UserControl>
